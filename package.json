{
  "name": "ng-dadf",
  "version": "0.0.0",
  "license": "MIT",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run env -s && ng serve --aot --port 4000",
    "build": "npm run env -s && ng build --resourcesOutputPath=resources --optimization=true --progress=true",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "env": "ngx-scripts env npm_package_version",
    "docs": "hads ./docs -o",
    "e2e": "npm run env -s && ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/forms": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "@ngx-rocket/scripts": "^3.0.2",
    "@ngx-translate/core": "^11.0.1",
    "@swimlane/ngx-charts": "^10.1.0",
    "angular-gridster2": "^7.1.0",
    "core-js": "2.6.5",
    "d3": "^5.9.2",
    "d3-shape": "^1.3.5",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "ng2-cookies": "^1.0.12",
    "npm": "^6.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "semantic-ui": "^2.4.2",
    "socket.io-client": "^2.2.0",
    "sockjs-client": "^1.3.0",
    "stompjs": "^2.3.3",
    "tslib": "^1.9.3",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.13",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.13.4",
    "codelyzer": "^5.0.0",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "stylelint": "~9.6.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "typescript": "3.1.1"
  }
}
